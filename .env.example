# Tomo Environment Variables
# Copy this file to .env and fill in your actual API keys

# Claude API Key (for AI-powered move generation and chat)
EXPO_PUBLIC_CLAUDE_API_KEY=your_claude_api_key_here

# OpenWeatherMap API Key (for real-time weather data)
EXPO_PUBLIC_WEATHER_API_KEY=your_openweather_api_key_here

# Google Places API Key (for nearby places and transit data)
EXPO_PUBLIC_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
